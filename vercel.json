{
    "version": 2,
    "builds": [
      {
        "src": "Backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/send-email",
        "dest": "Backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }